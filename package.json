{
  "name": "mathcenterapp",
  "main": "bin/www",
  "browserslist": [
    "> 1%",
    "ie > 9"
  ],
  "scripts": {
    "start": "node ./bin/www",
    "main-storage:migration:create": "sequelize migration:create --name",
    "main-storage:migration:run": "sequelize db:migrate",
    "main-storage:migration:undo": "sequelize db:migrate:undo",
    "postinstall": "npm run clean && node node_modules/node-sass/scripts/install.js && npm rebuild node-sass && webpack --config ./webpack.production.config.js",
    "clean": "rimraf dist",
    "production": "cross-env NODE_ENV=production npm run start",
    "dev:client": "cross-env NODE_ENV=dev npm run start -- -dev-client",
    "dev:server": "webpack --config ./webpack.production.config.js && cross-env NODE_ENV=dev nodemon -w ./ -x npm run start",
    "test:unit": "jest -o --verbose || true",
    "test:unit:all": "jest",
    "test:integration:all": "jest ./test/integration/**"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "cross-env": "^5.1.3",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^18.1.0",
    "nodemon": "^1.9.2",
    "puppeteer": "^1.1.1",
    "react-test-renderer": "^15.4.1",
    "redux-logger": "^2.6.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "axios": "^0.11.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "body-parser": "^1.15.1",
    "breakpoint-sass": "^2.7.0",
    "chalk": "^1.1.3",
    "compass-mixins": "^0.12.10",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.2",
    "connect-redis": "^3.3.3",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "google-libphonenumber": "^3.0.10",
    "googleapis": "^9.0.0",
    "helmet": "^3.9.0",
    "html-webpack-plugin": "^2.26.0",
    "jsonwebtoken": "^7.4.0",
    "memory-cache": "^0.1.6",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.4",
    "mongoose": "^4.10.5",
    "morgan": "^1.7.0",
    "node-sass": "^3.7.0",
    "passport": "^0.3.2",
    "passport-azure-ad-oauth2": "^0.0.4",
    "pg": "^4.5.5",
    "pg-hstore": "^2.3.2",
    "postcss-loader": "^2.0.5",
    "ramda": "^0.24.0",
    "react": "^15.1.0",
    "react-datepicker": "^0.47.0",
    "react-dom": "^15.1.0",
    "react-mixin": "^3.0.5",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
