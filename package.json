    "faker": "^4.1.0",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "nodemon": "^1.9.2",
    "prettier": "^1.13.5",
    "puppeteer": "^1.1.1",
    "react-test-renderer": "^15.4.1",
    "redux-logger": "^2.6.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  },
